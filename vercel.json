{
  "version": 2,
  "builds": [
    {
      "src": "api/send-message.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/send-message.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
